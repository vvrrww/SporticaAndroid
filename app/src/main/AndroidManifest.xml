<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sportica">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TournamentPlayerFormActivity"></activity>
        <activity
            android:name=".TournamentSportActivity"
        <activity android:name=".WelcomeActivity"></activity>
        <activity android:name=".ChulaSSOActivity" />
        <activity android:name=".TournamentSportActivity"
            android:parentActivityName=".TournamentDetailActivity" />
        <activity
            android:name=".TournamentMedalsActivity"
            android:parentActivityName=".TournamentDetailActivity" />
        <activity
            android:name=".TournamentPlayerActivity"
            android:parentActivityName=".TournamentDetailActivity" />
        <activity
            android:name=".TournamentContactActivity"
            android:parentActivityName=".TournamentDetailActivity" />
        <activity
            android:name=".TournamentDetailActivity"
            android:parentActivityName=".NavActivity" />
        <activity android:name=".ChulaSSOActivity" />
        <activity
            android:name=".NewsDetailActivity"
            android:parentActivityName=".NavActivity" />
        <activity android:name=".NavActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>